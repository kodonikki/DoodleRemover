{
    "manifest_version": 3,
    "name": "Doodle remover",
    "version": "1.0",
    "description": "Delete Youtube doodles",
    "author": "Maru",
    "browser_action":{
        "default_tittle": "Doodle remover",
        "default_popup": "index.html"
    },
    "content_scripts":[
        {
            "css": 
            ["style.css"],
            "matches":[
                "https://www.youtube.com/"
            ]
        }
    ]
    }
